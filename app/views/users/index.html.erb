
      <%= render 'template/sidemenu' %>
  	  <div class="col-xs-9" >
  	  	<h2>Users</h2>
  	  	<table class="table table-hover table-inverse">
  	  		<thead>
  	  			<tr>
  	  				<th>image</th>
  	  				<th>name</th>
  	  				<th></th>
  	  				<th colspan="3"></th>
  	  			</tr>
  	  		</thead>
  	  		<tbody>
  	  			<% @users.each do |f| %>
  	  			<tr>
  	  			<td>
              <%= link_to "", user_path(f.id) %>
          <%= attachment_image_tag f, :profile_image, format:'jpeg', fallback: "no_image.jpg", size: "40x40" %>
  	  		    </td>
  	  			<td>
  	  				<%= f.name %>
  	  			</td>
  	  			<td>
  	  			</td>
            <td>
            <%= link_to "show", user_path(f.id)%>
            </td>
  	  			</tr>
  	  			<% end %>

  	  		</tbody>
  	  	</table>
  	  </div>
  	</div>


